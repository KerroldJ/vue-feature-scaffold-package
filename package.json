{
  "name": "vue-feature-scaffold",
  "version": "0.1.0",
  "description": "CLI tool to generate feature-based Vue.js architecture for Laravel projects",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "vue-feature": "./dist/cli.js"
  },
  "files": [
    "dist",
    "templates"
  ],
  "scripts": {
    "dev": "tsc --watch",
    "build": "tsc",
    "prepublishOnly": "npm run build",
    "test": "vitest",
    "lint": "eslint src --ext .ts",
    "generate": "node dist/cli.js generate"
  },
  "keywords": [
    "vue",
    "laravel",
    "cli",
    "scaffold",
    "generator",
    "feature-based",
    "architecture",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "typescript": "^5.3.3",
    "vitest": "^1.0.4",
    "eslint": "^8.56.0",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0"
  },
  "dependencies": {
    "commander": "^11.1.0",
    "chalk": "^5.3.0",
    "ora": "^8.0.1",
    "prompts": "^2.4.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
