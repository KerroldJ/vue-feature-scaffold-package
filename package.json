{
  "name": "vue-feature-scaffold",
  "version": "0.2.0",
  "description": "CLI tool to generate feature-based Vue.js architecture for any Vue.js project with TypeScript support",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "vue-feature": "./dist/cli.js"
  },
  "files": [
    "dist",
    "templates",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "tsc --watch",
    "build": "tsc",
    "prepublishOnly": "npm run build",
    "test": "vitest",
    "lint": "eslint src --ext .ts",
    "generate": "node dist/cli.js generate"
  },
  "keywords": [
    "vue",
    "vuejs",
    "vue3",
    "laravel",
    "inertia",
    "nuxt",
    "vite",
    "cli",
    "scaffold",
    "generator",
    "feature-based",
    "architecture",
    "typescript",
    "code-generator",
    "boilerplate",
    "template"
  ],
  "author": "KerroldJ",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KerroldJ/vue-feature-scaffold-package.git"
  },
  "bugs": {
    "url": "https://github.com/KerroldJ/vue-feature-scaffold-package/issues"
  },
  "homepage": "https://github.com/KerroldJ/vue-feature-scaffold-package#readme",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "typescript": "^5.3.3",
    "vitest": "^1.0.4",
    "eslint": "^8.56.0",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0"
  },
  "dependencies": {
    "commander": "^11.1.0",
    "chalk": "^5.3.0",
    "ora": "^8.0.1",
    "prompts": "^2.4.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
